/* 悬浮按钮 */

.buttom {
  position: fixed;
  bottom: 70rpx;
  right: 60rpx;
  width: 98rpx;
  height: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  z-index: 1001;
  color: #f8f8f9;
  background-color: #39b54a;
  border-radius: 50%;
}

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.drawer_box {
  overflow: hidden;
